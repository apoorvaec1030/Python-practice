      ],
      "source": [
        "import pandas as pd\n",
        "person=pd.DataFrame({\"id\":[1,2,5,3],\"email\":[\"a@b.com\",\"c@d.com\",\"a@b.com\",\"a@c.com\"]})\n",
        "\n",
        "def duplicate_emails(person):\n",
        "  freq=dict()\n",
        "  for email in person['email']:\n",
        "    freq[email]=freq.get(email,0)+1\n",
        "\n",
        "  dup_emails=[k for k,v in freq.items() if v>1]\n",
        "  return pd.DataFrame({'Email':dup_emails})\n",
        "\n",
        "\n",
        "\n",
        "\n",
        "\n",
        "duplicate_emails(person)"
      ]
    },
    {
      "cell_type": "code",
      "source": [
        "def duplicate_emails(person):\n",
        "  return pd.DataFrame({'Email':person.email.value_counts()[lambda x:x>1].index.to_list()})\n",
        "\n",
        "duplicate_emails(person)"
      ],
